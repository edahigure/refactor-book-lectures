<div id="<%= dom_id lecture %>">
  <p>
    <strong>Name:</strong>
    <%= lecture.name %>
  </p>

  <p>
    <strong>Image url:</strong>
    <%= lecture.image_url %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= lecture.description %>
  </p>

  <p>
    <strong>Web link:</strong>
    <%= lecture.web_link %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= lecture.price %>
  </p>

  <p>
    <strong>Removed:</strong>
    <%= lecture.removed %>
  </p>

  <p>
    <strong>Teacher:</strong>
    <%= lecture.teacher_id %>
  </p>

</div>
